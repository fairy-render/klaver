{
  "compilerOptions": {
    "lib": [],
    "typeRoots": ["./@types"],
    "jsxImportSource": "@wilbur/template",
    "jsx": "preserve",
    "target": "ES2022",
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "allowImportingTsExtensions": true,
    "baseUrl": "."
  }
}
